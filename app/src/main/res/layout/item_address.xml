<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/rl_setting_types"
                style="@style/bg_white_style"
                android:layout_marginTop="@dimen/y4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y248">

    <TextView
            android:id="@+id/tv_alias"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="@dimen/x32"
            android:layout_marginTop="@dimen/y40"/>

    <TextView
            android:id="@+id/tv_setting_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/x28"
            android:textColor="@color/black"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/tv_alias"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/y32"
            android:layout_toLeftOf="@+id/ib_delete"
            android:lineSpacingExtra="@dimen/x6"
            android:maxHeight="@dimen/y88"
            android:maxLines="2"/>

    <ImageButton
            android:id="@+id/ib_delete"
            style="@style/ib_style"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/y24"
            android:src="@mipmap/icon_delete"/>


</RelativeLayout>